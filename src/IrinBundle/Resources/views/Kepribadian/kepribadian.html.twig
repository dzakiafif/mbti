                perc = ['p0119' ,'p0118','p0117','p0116','p0115','p0114','p0113','p0112','p0111','p0110','p0109',,'p0108','p0107','p0106','p0105'], 
                    str='',
                    arr= new Array(),
                    dmValue = 0,
                    introValue = 0,
                    intuValue = 0,
                    thinValue = 0,
                    feelValue = 0,
                    judgValue = 0,
                    percValue = 0,
                    sensiValue = 0,
                    test = $('input[type=text][name=introvert]'),
                    ekstrovert = $('input[type=text][name=ekstrovert]'),
                    sensing = $('input[type=text][name=sensing]'),
                    intuition = $('input[type=text][name=intuition]'),
                    thinking = $('input[type=text][name=thinking]'),
                    feeling = $('input[type=text][name=feeling]'),
                    judging = $('input[type=text][name=judging]'),
                    perceiving = $('input[type=text][name=perceiving]');

            for (i = 1; i <= numberOfRadio; i++) {
                $("[name=group" + i + "]:checked").map(function () {
                    arr[i-1]=$(this).val();
                });
            }

            $.each(arr, function(index, value) {
                $.each(demam, function(probIndex, prob) {
                    if (value == prob) {
                        dmValue++;
                    }
                });
                $.each(intr, function (probIndex, prob) {
                    if(value == prob) {
                        introValue++;
                    }
                });
                $.each(sens, function (probIndex, prob) {
                   if(value == prob) {
                       sensiValue++;
                   }
                });
                $.each(intu, function (probIndex, prob) {
                    if(value == prob) {
                        intuValue++;
                    }
                });
                $.each(thin, function(probIndex, prob) {
                   if(value == prob) {
                        thinValue++;
                   }         
                });
                $.each(feel, function(probIndex, prob) {
                   if(value == prob) {
                        feelValue++;
                   }     
                });
                $.each(judg, function(probIndex, prob) {
                   if(value == prob) {
                        judgValue++;
                   }     
                });
                $.each(perc, function(probIndex, prob) {
                   if(value == prob) {
                        percValue++;
                   }     
                });
            });

            // DO PRESENTAGE
            var dmPrec = Math.round((dmValue/15) * 100);

            var intrPrec = Math.round((introValue/15) * 100);

            var senPrec = Math.round((sensiValue/15) * 100);

            var intuiPrec = Math.round((intuValue/15) * 100);

            var thinPrec = Math.round((thinValue/15) *100);

            var feelPrec = Math.round((feelValue/15) * 100);

            var judPrec = Math.round((judgValue/15) * 100);

            var percPrec = Math.round((percValue/15) * 100);

            test.val(dmPrec + '%');

            ekstrovert.val(intrPrec + '%');

            sensing.val(senPrec + '%');

            intuition.val(intuiPrec + '%');

            thinking.val(thinPrec + '%');

            feeling.val(feelPrec + '%');

            judging.val(judPrec + '%');

            perceiving.val(percPrec + '%');

